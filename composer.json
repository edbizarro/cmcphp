{
  "name": "edbizarro/cmcphp",
  "type": "library",
  "description": "PHP wrapper for CoinMarketCap API.",
  "keywords": [
    "CoinMarketCap",
    "Coint Market Cap",
    "nano",
    "ethereum",
    "bitcoin"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo Bizarro",
      "email": "edbizarro@gmail.com",
      "homepage": "https://bizarro.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.13",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2 || ^3"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Edbizarro\\Cmc\\": "src/"
    }
  }
}
